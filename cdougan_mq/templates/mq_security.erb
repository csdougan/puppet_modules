SET CHLAUTH('*') TYPE(ADDRESSMAP) ADDRESS('*') USERSRC(NOACCESS) DESCR('lock all') ACTION(REPLACE) 
SET CHLAUTH(<%= @mq_channel_bma %>) TYPE(ADDRESSMAP) ADDRESS(*) USERSRC(CHANNEL) ACTION(REPLACE)
SET CHLAUTH(<%= @mq_channel_bma %>) TYPE(BLOCKUSER) USERLIST(*none) ACTION(REPLACE)
ALTER AUTHINFO(SYSTEM.DEFAULT.AUTHINFO.IDPWOS) AUTHTYPE(IDPWOS) ADOPTCTX(YES) CHCKCLNT(OPTIONAL)
REFRESH SECURITY TYPE(CONNAUTH)
SET CHLAUTH(<%= @mq_channel_main %>) TYPE(USERMAP) CLNTUSER(<%= @mq_user %>) USERSRC(MAP) MCAUSER(<%= @mq_user %>) ACTION(REPLACE)
SET CHLAUTH(<%= @mq_channel_main %>) TYPE(ADDRESSMAP) ADDRESS('1.3.*') USERSRC(CHANNEL) ACTION(REPLACE)
SET CHLAUTH() TYPE(ADDRESSMAP) ADDRESS('1.2.*') USERSRC(CHANNEL) ACTION(REPLACE)
REFRESH SECURITY (*)
END
